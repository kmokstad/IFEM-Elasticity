<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<!-- In-homogeneous Dirichlet condition test, X-direction. !-->

<simulation>

  <geometry dim="3" X0="-1.0 -0.7 -0.8" Lx="2.0" Ly="1.4" Lz="1.6">
    <raiseorder patch="1" u="1" v="1" w="1"/>
    <refine patch="1" u="3" v="1" w="2"/>
    <topologysets>
      <set name="Udir" type="face">
        <item patch="1">1 2</item>
      </set>
      <set name="P1" type="vertex">
        <item patch="1">1</item>
      </set>
      <set name="P2" type="vertex">
        <item patch="1">2</item>
      </set>
      <set name="P3" type="vertex">
        <item patch="1">3</item>
      </set>
    </topologysets>
  </geometry>

  <boundaryconditions>
    <dirichlet set="Udir" component="1" type="expression">
      0.1*x*(1.0-(y/0.7)^2)
    </dirichlet>
    <dirichlet set="P1" component="23"/>
    <dirichlet set="P2" component="3"/>
    <dirichlet set="P3" component="3"/>
  </boundaryconditions>

  <discretization>
    <nGauss>3 4</nGauss>
  </discretization>

</simulation>
